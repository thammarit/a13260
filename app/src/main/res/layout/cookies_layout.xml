<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="10dp">

    <ImageView
        android:layout_width="120dp"
        android:layout_height="70dp"
        android:id="@+id/cookieResultImg"
        />
    <TextView
        android:layout_width="250dp"
        android:layout_height="35dp"
        android:layout_marginTop="10dp"
        android:id="@+id/resultTimeStamp"
        android:layout_alignBottom="@+id/cookieResultImg"
        android:layout_alignStart="@+id/resultFinalText" />

    <TextView
        android:layout_width="250dp"
        android:layout_height="35dp"
        android:paddingTop="10dp"
        android:id="@+id/resultFinalText"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/cookieResultImg" />

</RelativeLayout>
